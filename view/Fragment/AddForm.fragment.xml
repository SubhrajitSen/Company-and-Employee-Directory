<core:FragmentDefinition
   xmlns:mvc="sap.ui.core.mvc"
    xmlns:f="sap.ui.layout.form"
    xmlns:l="sap.ui.layout"
    xmlns:u="sap.ui.unified"
    xmlns:core="sap.ui.core"
    xmlns:tnt="sap.tnt"
    xmlns="sap.m" >
   <ViewSettingDialog
     id="EmployeeForm" title="Employee New Form">
       <f:SimpleForm 
                id="SimpleFormChange353"
			editable="true"
			layout="ResponsiveGridLayout"
			labelSpanXL="2"
			labelSpanL="2"
			labelSpanM="2"
			labelSpanS="12"
			adjustLabelSpan="false"
			emptySpanXL="0"
			emptySpanL="0"
			emptySpanM="0"
			emptySpanS="0"
			columnsXL="1"
			columnsL="1"
			columnsM="1"
			singleContainerFullSize="false" >  
            <f:content>
            <core:Title text="Employee Information"/>
        	<Label text="Employee ID"/>
              <Input
                  
            id="EmployeeID"
			/>
            <Label text="Title Of Courtesy" />
		<Input
          
            id="TitleOfCourtesy"
			/>
             <Label text="First Name" />
		<Input
          
            id="FirstName"
			/>
             <Label text="Last Name" />
		<Input
          
            id="LastName"
			/>
            <Label text="Title" />
		<Input
          
         id="Title"
			/> 
            <Label text="City" />
            <Input
              
            id="City"
			/>
            <Label text="Hire Date" />
            <Input
               type="Number"
            id="HireDate"
			/>
            <Label text="Birth Date" />
             <Input
              type="Number" 
             id="BirthDate"
			/>
            <Label text="Address" />
            <Input
              
            id="Address"
			/>
            <Label text="Postal Code" />
            <Input
             type="Number" 
            id="PostalCode"
			/>
            <Label text="Country" />
            <Input
              
            id="Country"
			/>
            <Label text="Home Phone" />
            <Input
              type="Number"
            id="HomePhone"
			/>
            <Label text="Notes" />
              <Input
                
            id="Notes"  	/>
            <Label text="Photo Path" />
              <Input
                
             id="PhotoPath"
			/>
            <Button id="save" text="Save" type="Emphasized"  press="handleSavePress" />
					<Button id="cancel" text="Cancel"  press="handleCancelPress" />
                    <Button id="reset" text="Reset"  press="handleResetPress" />
    </f:content>
                    </f:SimpleForm>
   </ViewSettingDialog>
</core:FragmentDefinition>